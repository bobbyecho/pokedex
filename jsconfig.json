{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components": ["src/components/index.js"],
      "@components/*": ["src/components/*"],
      "@themes/*": ["src/themes/*"],
      "@src/*": ["src/*"],
      "@features/*": ["src/features/*"],
